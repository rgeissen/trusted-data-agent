# Trusted Data Agent - Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# JWT Authentication (OPTIONAL - auto-generated if not set)
# The JWT secret key is automatically generated and stored in tda_keys/jwt_secret.key
# Only set this environment variable if you need to override the stored key
# TDA_JWT_SECRET_KEY=your_jwt_secret_key_here

# JWT Token Settings
# TDA_JWT_EXPIRY_HOURS=24
# TDA_PASSWORD_MIN_LENGTH=8
# TDA_MAX_LOGIN_ATTEMPTS=5
# TDA_LOCKOUT_DURATION_MINUTES=15

# AWS Bedrock (if using)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# OpenAI (if using)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (if using)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Application Settings
SECRET_KEY=your_secret_key_for_session_encryption
